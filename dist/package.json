{
  "name": "@kaineee/nier-automata-ui-library",
  "version": "0.2.9",
  "description": "react ui library component based of nier automata settings ui",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "buildlib": "npm run clean && rollup -c  && cp package.json README.md ./dist ",
    "clean": "rm -rf dist",
    "publish": "npm publish ./dist --access public"
  },
  "author": "Kaine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kndgy/NieR-Automata-Design-System/issues"
  },
  "homepage": "https://nier-ui-storybook.netlify.app/",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@kaineee/nier-automata-ui-library": "^0.2.5",
    "@rollup/plugin-json": "^4.1.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-named-exports-order": "^0.0.2",
    "node-sass": "^7.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "rollup": "^2.75.7",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "sass": "^1.53.0",
    "storybook-addon-react-router-v6": "^0.1.10",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  },
  "dependencies": {
    "framer": "^2.2.0",
    "react-shuffle-text": "^1.0.6",
    "styled-components": "^5"
  },
  "peerDependencies": {
    "styled-components": "^5",
    "react-shuffle-text": "^1.0.6"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
